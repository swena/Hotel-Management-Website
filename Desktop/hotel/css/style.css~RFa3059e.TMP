 #menu1, #menu1 ul, #menu1 li{
padding:0;
margin:0;
}
#menu1
{
	width:30%;
	float:left;
}
#menu1 a
{
	display:block;
	width:280px;
height: 60px;

}
#menu1 li{
list-style: none;
position: relative;
width:260px;
height: 60px;
padding:0 20px;
background-color: black;
line-height: 57px;
cursor:pointer;
font-family:"pala";
font-size:30px;
text-align:center;
border-bottom-style:solid;
border-bottom-color:#999;
color: white;

}
#menu1 li a{
text-decoration:none;
color: white;

}

#menu1 li:hover{
background-color:red;
}
#menu1 ul ul{
position: absolute;
top:0;
left:300px;
visibility:hidden;
}
#menu1 ul li:hover ul{
visibility:visible;
} 
#menu1 ul li:hover ul ul{
visibility:hidden;
}
#menu1 ul ul ul{
position: absolute;
top:0;
left:300px;
visibility:hidden;
}
#menu1 ul ul li:hover ul{
visibility:visible;
}
