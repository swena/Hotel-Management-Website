 #menu1, #menu1 ul, #menu1 li{
padding:0;
margin:0;
}
#menu1 li{
list-style: none;
position: relative;
width:200px;
height: 50px;
padding:0 20px;
background-color: black;
line-height: 30px;
cursor:pointer;
font-family:"starNight";
font-size:50px;
}
#menu1 li a{
text-decoration:none;
color: white;
}
#menu1 li:hover{
background-color:red;
}
#menu1 ul ul{
position: absolute;
top:0;
left:160px;
visibility:hidden;
}
#menu1 ul li:hover ul{
visibility:visible;
}